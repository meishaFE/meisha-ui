  "scripts": {
    "bootstrap": "yarn || npm i",
    "build:dev": "webpack --config build/webpack.dist.dev.conf.js",
    "build:prod": "webpack --config build/webpack.dist.prod.conf.js",
    "build:entry": "node build/bin/build-entry.js",
    "build:theme": "node build/bin/gen-scssfile && gulp build --gulpfile build/build-style.js",
    "build": "npm run clean && npm run lint && npm run build:dev && npm run build:prod && npm run build:theme",
    "clean": "rimraf lib && rimraf test/**/coverage",
    "dev": "npm run bootstrap && node ./build/dev-server.js",
    "dist": "npm run clean && npm run lint && npm run build:theme && npm run build:prod",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test:theme": "gulp test --gulpfile build/build-style.js",
    "test": "npm run lint && npm run test:theme && npm run unit",
    "gen": "node ./build/bin/gen-file.js",
    "prepublish": "npm run dist"
  },
